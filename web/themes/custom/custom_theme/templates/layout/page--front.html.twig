
<div class="wrapper">
	{% if page.header %}
		<header class="header_area">
			<nav class="navbar navbar-expand-lg menu_one menu_eight">
				<div class="container">
					{{ page.header }}
				</div>
			</nav>
		</header>
	{% endif %}

	{{ page.content }}
	{% if (page.footer or page.bottom_footer)  %}
		<footer class="footer_area footer_dark_one footer_dark_ten pt_120">
			{% if page.footer %}
				<div class="footer_top">
					<div class="container">
						<div class="row">
							{{ page.footer }}
						</div>
					</div>
				</div>
			{% endif %}
			{% if page.bottom_footer %}
				<div class="footer_bottom dark_f_bottom">
					<div class="container">
						<div class="row align-items-center">
							{{ page.bottom_footer }}
						</div>
					</div>
				</div>
			{% endif %}
		</footer>
	{% endif %}
</div>
